<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane prefWidth="335.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <content>
        <GridPane hgap="3" vgap="5">

            <Label prefHeight="17.0" prefWidth="162.0" text="Дата от:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
            <DatePicker fx:id="dateFrom" prefHeight="25.0" prefWidth="270.0" GridPane.columnIndex="1" GridPane.fillWidth="false" GridPane.rowIndex="0" />

            <Label prefHeight="17.0" prefWidth="126.0" text="Дата до:" GridPane.columnIndex="0" GridPane.rowIndex="1" />
            <DatePicker fx:id="dateTo" prefHeight="25.0" prefWidth="270.0" GridPane.columnIndex="1" GridPane.fillWidth="false" GridPane.rowIndex="1" />

            <columnConstraints>
                <ColumnConstraints maxWidth="144.0" minWidth="81.0" percentWidth="26.0" prefWidth="81.0" />
                <ColumnConstraints hgrow="ALWAYS" maxWidth="324.0" minWidth="200.0" percentWidth="74.0" prefWidth="324.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
            </rowConstraints>
        </GridPane>
    </content>

    <buttonTypes>
        <ButtonType fx:id="okayButtonType" buttonData="OK_DONE" text="Приложи" />
        <ButtonType buttonData="CANCEL_CLOSE" text="Затвори" />
    </buttonTypes>

</DialogPane>
