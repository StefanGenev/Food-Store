<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<StackPane xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:controller="com.foodstore.controllers.LoadController"
           prefHeight="400.0" prefWidth="600.0">
    <TableView fx:id="tableView" prefHeight="600.0" prefWidth="985.0" tableMenuButtonVisible="true">
        <columns>
            <TableColumn fx:id="colProductName" maxWidth="4000.0" prefWidth="75.0" text="Продукт"/>
            <TableColumn fx:id="colQuantity" maxWidth="4000.0" prefWidth="75.0" text="Количество"/>
            <TableColumn fx:id="colDateOfLoading" maxWidth="4000.0" prefWidth="75.0" text="Дата на зареждане"/>
        </columns>
        <VBox.margin>
            <Insets left="10.0" right="5.0"/>
        </VBox.margin>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</StackPane>